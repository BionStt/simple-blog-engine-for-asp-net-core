@model GoogleAnalyticsSettings

@{if(Model.IsEnabled) {
	<script async src="https://www.googletagmanager.com/gtag/js?id=@Model.TrackingID"></script>
	<script>
	  window.dataLayer = window.dataLayer || [];
	  function gtag(){dataLayer.push(arguments)};
	  gtag('js', new Date());
	  gtag('config', '@Model.TrackingID');
	</script>
}}